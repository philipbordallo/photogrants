{
  "name": "photogrants",
  "version": "0.3.0",
  "description": "",
  "author": "Philip Bordallo",
  "license": "ISC",
  "private": true,
  "homepage": "https://github.com/philipbordallo/photogrants#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philipbordallo/photogrants.git"
  },
  "scripts": {
    "start": "npm run dev",
    "build": "npm run cleanup && npm run data-prod && npm run prod",
    "cleanup": "rimraf dist/",
    "data": "npm run data-test && npm run data-build",
    "data-build": "NODE_ENV=data babel-node data/build",
    "data-test": "npm run test spec/data/data_spec.js",
    "data-test-min": "npm run test -- --tap spec/data/data_spec.js | tap-min",
    "data-prod": "npm run data-test-min && npm run data-build",
    "dev": "NODE_ENV=development webpack-dev-server --config ./config/webpack.config.dev.js --progress",
    "prod": "NODE_ENV=production webpack --config ./config/webpack.config.prod.js",
    "server": "http-server dist/",
    "test": "NODE_ENV=test ava",
    "test-watch": "npm test -- --watch --verbose spec/"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "./config/babel.config.js"
    ]
  },
  "browserslist": [
    "last 1 Android version",
    "Chrome >= 55",
    "last 2 ChromeAndroid versions",
    "Firefox >= 52",
    "last 2 FirefoxAndroid versions",
    "Safari >= 10",
    "Edge >= 13",
    "Explorer >= 11",
    "iOS >= 9.3",
    "last 2 Opera versions",
    "last 2 OperaMini versions",
    "last 2 OperaMobile versions",
    "last 2 Samsung versions"
  ],
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "ava": "^0.21.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.1",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "csswring": "^6.0.1",
    "eslint": "^4.7.2",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "handlebars": "^4.0.10",
    "handlebars-loader": "^1.6.0",
    "html-webpack-plugin": "^2.30.1",
    "http-server": "^0.10.0",
    "jsonschema": "^1.1.1",
    "local-domains": "^1.2.3",
    "postcss-custom-media": "^6.0.0",
    "postcss-custom-properties": "^6.1.0",
    "postcss-discard-comments": "^2.0.4",
    "postcss-font-family-system-ui": "^2.0.1",
    "postcss-font-smoothing": "^0.1.0",
    "postcss-font-weights": "^4.0.0",
    "postcss-hexrgba": "^1.0.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-media-minmax": "^3.0.0",
    "postcss-nesting": "^4.2.1",
    "postcss-normalize-whitespace": "^4.0.0-rc.0",
    "postcss-pseudoelements": "^5.0.0",
    "postcss-pxtorem": "^4.0.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "tap-min": "^1.2.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "currency-symbol-map": "^4.0.2",
    "listify": "^1.0.0",
    "moment": "^2.18.1",
    "normalize.css": "^7.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "smoothscroll-polyfill": "^0.4.0",
    "whatwg-fetch": "^2.0.3"
  }
}
