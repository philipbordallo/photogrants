{
  "name": "photogrants",
  "version": "0.10.1",
  "description": "",
  "author": "Philip Bordallo",
  "license": "ISC",
  "private": true,
  "homepage": "https://github.com/philipbordallo/photogrants#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philipbordallo/photogrants.git"
  },
  "scripts": {
    "start": "npm run dev",
    "build": "npm run cleanup && npm run data:prod && npm run prod",
    "cleanup": "rimraf dist/",
    "data": "npm run data:test && npm run data:build",
    "data:build": "NODE_ENV=data babel-node data/build",
    "data:test": "npm run test spec/data/data.test.js",
    "data:prod": "npm run data:test –– ––tap  && npm run data:build",
    "dev": "NODE_ENV=development webpack serve --config ./configs/webpack.config.js --progress",
    "prod": "NODE_ENV=production webpack --config ./configs/webpack.config.js",
    "server": "http-server dist/",
    "test": "NODE_ENV=test ava",
    "test:watch": "npm test -- --watch spec/"
  },
  "ava": {
    "babel": true,
    "require": [
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      "./configs/babel.config.js"
    ]
  },
  "eslintConfig": {
    "extends": "./configs/eslint.config.js"
  },
  "browserslist": [
    "last 1 Android version",
    "Chrome >= 55",
    "last 2 ChromeAndroid versions",
    "Firefox >= 52",
    "last 2 FirefoxAndroid versions",
    "Safari >= 10",
    "Edge >= 13",
    "Explorer >= 11",
    "iOS >= 9.3",
    "last 2 Opera versions",
    "last 2 OperaMini versions",
    "last 2 OperaMobile versions",
    "last 2 Samsung versions"
  ],
  "devDependencies": {
