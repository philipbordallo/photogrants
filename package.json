    "@babel/preset-react": "^7.12.10",
    "@babel/register": "^7.12.10",
    "@philipbordallo/eslint-config-react": "^3.2.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "autoprefixer": "^10.1.0",
    "ava": "^3.15.0",
